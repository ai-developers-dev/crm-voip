{
  "mcpServers": {
    "convex": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "convex@latest",
        "mcp",
        "start"
      ],
      "env": {}
    },
    "twilio": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@twilio-alpha/mcp",
        "${TWILIO_ACCOUNT_SID}/${TWILIO_API_KEY}:${TWILIO_API_SECRET}",
        "--services",
        "voice_v1"
      ],
      "env": {
        "TWILIO_ACCOUNT_SID": "${TWILIO_ACCOUNT_SID}",
        "TWILIO_API_KEY": "${TWILIO_API_KEY}",
        "TWILIO_API_SECRET": "${TWILIO_API_SECRET}"
      }
    }
  }
}